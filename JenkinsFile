pipeline{
    agent any
     
    stages {
        stage('Descarga de Librerias Node Modules'){
            steps{
                bat 'npm install'
            }
        }
        stage('Pruebas Unitarias MochaJS'){
            steps{
                bat 'npm run test'
            }
        }
        stage('Armar artefactos'){
            steps{
                bat 'node_modules\\.bin\\gulp'
            }
        }

    }
}